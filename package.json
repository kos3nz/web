{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro preview",
    "build": "astro build",
    "preview": "wrangler pages dev ./dist",
    "lint": "run-p lint:es lint:style",
    "lint:es": "eslint \"**/*.{js,ts,jsx,tsx,astro,mdx}\" --cache --cache-location node_modules/.cache/eslint/.cache",
    "lint:style": "stylelint \"src/**/*.{css,astro}\" --cache --cache-location node_modules/.cache/stylelint/.cache",
    "lint:fix": "run-p lint:fix:*",
    "lint:fix:es": "pnpm lint:es --fix",
    "lint:fix:style": "pnpm lint:style --fix",
    "format:check": "prettier --check \"**/*.{css,js,ts,jsx,tsx,astro,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{css,js,ts,jsx,tsx,astro,mdx}\" --cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/react": "^3.0.9",
    "@astrojs/solid-js": "^4.0.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.1.1",
    "@hookform/resolvers": "^3.1.0",
    "@kobalte/core": "^0.9.6",
    "@motionone/solid": "^10.16.2",
    "@nanostores/react": "^0.7.0",
    "@nanostores/solid": "^0.3.2",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "astro": "^4.3.2",
    "astro-compress": "^2.2.9",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "framer-motion": "^10.12.16",
    "nanostores": "^0.9.3",
    "node-html-parser": "^6.1.5",
    "radash": "^10.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-use": "^17.4.0",
    "react-use-measure": "^2.1.1",
    "remove-markdown": "^0.5.0",
    "solid-js": "^1.7.5",
    "tailwind-merge": "^1.12.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.56.2",
    "@types/remove-markdown": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "daisyui": "^2.51.6",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-solid": "^0.13.1",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "eslint-plugin-vitest": "^0.3.21",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-preset-minify": "^7.0.0",
    "rehype-pretty-code": "^0.12.6",
    "rehype-slug": "^6.0.0",
    "shiki": "^0.14.7",
    "shikiji": "^0.10.2",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-high-performance-animation": "^1.10.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "src/**/*.{css,js,ts,jsx,tsx,mdx,astro}": [
      "prettier --write --cache"
    ],
    "src/**/*.{js,ts,jsx,tsx,mdx,astro}": [
      "eslint --fix --cache --cache-location node_modules/.cache/eslint/.cache"
    ],
    "src/**/*.{css,astro}": [
      "stylelint --fix --cache --cache-location node_modules/.cache/stylelint/.cache"
    ]
  }
}
